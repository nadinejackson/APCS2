public class QSolver
{
    public static String print(int[][] arr)
    {
	String ret = "";
	for (int i = 0; i < arr.length; i++){
	    for (int j = 0; j < arr[0].length; j++)
		ret += j + " ";
	    ret += "\n"
		}
	return ret;
    }
    public static int[][] solve()
    {
	return solve(new int[8][8], 0);
    }
